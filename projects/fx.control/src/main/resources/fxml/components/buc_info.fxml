            </Label>
            <Label text="%device.type" GridPane.rowIndex="4">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </Label>
            <Label fx:id="lblPN" text="Label" GridPane.columnIndex="2" />
            <Label fx:id="lblCounter" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Label fx:id="lblBuild" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <Label fx:id="lblVersion" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <Label fx:id="lblDeviceType" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="4" />
