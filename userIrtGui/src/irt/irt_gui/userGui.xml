<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="imagePath" class="java.lang.String">
		<constructor-arg value="/irt/irt_gui/images/logo.gif"/>
	</bean>

	<bean id="tabTitle" class="java.lang.String">
		<constructor-arg value="IRT Technologies"/>
	</bean>

	<bean id="demoPanel" class="irt.tools.panel.DemoPanel">
		<constructor-arg ref="imagePath"/>
		<constructor-arg ref="tabTitle"/>
	</bean>

</beans>