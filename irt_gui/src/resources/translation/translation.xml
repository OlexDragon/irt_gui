<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="english" class="resources.tools.ValueLabel">
		<property name="value" value="en,US"/>
		<property name="label" value="English (US)"/>
	</bean>

	<bean id="language2" class="resources.tools.ValueLabel">
		<property name="value" value="fr,CA"/>
		<property name="label" value="Français (CA)"/>
	</bean>

	<bean id="chinese" class="resources.tools.ValueLabel">
		<property name="value" value="cn,CN"/>
		<property name="label" value="中國的 (CN)"/>
	</bean>

	<bean id="languages" class="java.util.ArrayList">
		<constructor-arg>
			<list>
<!-- 			<ref bean="english" />
				<ref bean="chinese" /> -->	
			</list>
		</constructor-arg>
	</bean>

</beans>