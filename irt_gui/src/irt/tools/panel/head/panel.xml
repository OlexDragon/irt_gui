<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="backgroundColor" class="java.awt.Color">
		<constructor-arg>
			<value>0x0B</value>
		</constructor-arg>
		<constructor-arg>
			<value>0x17</value>
		</constructor-arg>
		<constructor-arg>
			<value>0x3B</value>
		</constructor-arg>
	</bean>

	<bean id="userPanel" class="javax.swing.JPanel">
		<property name="background" ref="backgroundColor" />
		<property name="layout"><null/></property>
	</bean>

	<bean id="rightPanelBackgroundColor" class="java.awt.Color">
		<constructor-arg>
			<value>0</value>
		</constructor-arg>
		<constructor-arg>
			<value>0x33</value>
		</constructor-arg>
		<constructor-arg>
			<value>0x33</value>
		</constructor-arg>
	</bean>

	<bean id="extraPanel" class="javax.swing.JPanel">
		<property name="background" ref="backgroundColor" />
		<property name="layout"><null/></property>
	</bean>
